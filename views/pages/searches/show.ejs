<!DOCTYPE html>
<html>

<head>
  <meta charset="UTF-8">
  <meta name="viewport" conent="width=device-width, initial-scale=1.0">
  <title>Personal Book Collection</title>
  <link rel="stylesheet" href="/style/reset.css">
  <link rel="stylesheet" href="/style/base.css">
  <link rel="stylesheet" href="/style/layout.css">
  <link rel="stylesheet" href="/style/module.css">
</head>

<body>
  <header>
    <div>
        <a href="/">
        <h1>My Book List</h1>
        </a>
    </div>
  </header>

  <main>
    <h2>Search Results</h4>
  <ul class="search-results">
    <% searchesResults.forEach(item => { %>
        <section>
          <img src="<%= item.img_url %>">
          <h3> <div class= "underline">Title: </div><%= item.title %></h3>
          <h3><div class= "underline">Author: </div><%= item.author %></h3>
          <h3> <div class= "underline">Description:</div> <%= item.description %></h3>
          <h3> <div class= "underline">ISBN:</div> <%= item.isbn %></span>
        </section>
    <% }) %>
  </ul>
</main>
</body>

</html>



